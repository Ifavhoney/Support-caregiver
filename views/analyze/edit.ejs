<% include ../partials/header.ejs %>

<div class="container">
  <div class="row">
    <div class="col-md-offset-3 col-md-6">
      <h1><%= title %></h1>
     
      <div class="form-group">
          <label for="patient">Patient</label
          ><input
            type="text"
            class="form-control"
            id="patientTXT"
            name="patient"
            required
          />
        </div>

      <form class="form" method="POST">
        <div class="form-group">
          <label for="caregiver">CareGiver</label
          ><input
            type="text"
            class="form-control"
            id="caregiverTextField"
            name="caregiver"
            required
          />
        </div>


      <div class="form-group">
          <label for="typeOfCare">typeOfCare</label
          ><input
            type="text"
            class="form-control"
            id="typeOfCareTXT"
            name="typeOfCare"
            required
          />
        </div>
'

        <div class="form-group">
          <label for="typeOfAbuse">typeOfAbuse</label
          ><input
            type="text"
            class="form-control"
            id="typeOfAbuseTXT"
            name="typeOfAbuse"
            required
          />
        </div>

        <div class="form-group">
          <label for="interventiondone">is Agressive</label
          ><input
            type="text"
            class="form-control"
            id="isAgressiveTXT"
            name="isAgressive"
            required
          />
        </div>

        <div class="form-group">
          <label for="behaviour">Recommended Phrases</label
          ><input
            type="text"
            class="form-control"
            id="recommendedPhrases"
            name="recommendedPhrases"
            required
          />
        </div>


        <div class="form-group">
          <label for="location">personalRedirection</label
          ><input
            type="text"
            class="form-control"
            id="personalRedirectionTXT"
            name="personalRedirection"
            required
          />
        </div>


         <div class="form-group">
          <label for="location">successRating</label
          ><input
            type="text"
            class="form-control"
            id="successRatingTXT"
            name="successRating"
            required
          />
        </div>
           <div class="form-group">
          <label for="location">dateTime</label
          ><input
            type="text"
            class="form-control"
            id="dateTimeTXT"
            name="dateTime"
            required
          />
        </div>



        <div class="form-group">
          <label for="comments">Comments</label
          ><input
            type="text"
            class="form-control"
            id="CommentsTXT"
            name="Comments"
            required
          />
        </div>


        
        
        <button type="submit" class="btn btn-primary">
          <i class="fa fa-edit    "></i>
          Submit
        </button>
        <a href="/health-list" class="btn btn-warning">
          <i class="fa fa-undo" aria-hidden="true"> Cancel </i>
        </a>
      </form>
    </div>
  </div>
</div>

<% include ../partials/footer.ejs %>
