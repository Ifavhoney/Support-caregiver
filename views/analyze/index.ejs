<%include ../partials/header.ejs %>

<div class="container">
  <div class="row">
    <div class="col-md-offset-3 col-md-6">
      <h1><%= title %></h1>
      <a class="btn btn-primary" href="/incoming-list/add" role="button">
        <i class="fa fa-plus-circle">ANALYZE PATIENT </i>
      </a>

      <table class="table">
        <thead class="thead-light">
          <tr>
            <th scope="col">#</th>
            <th scope="col">recommendedPhrases</th>
            <th scope="col">personalRedirection</th>
                        <th scope="col">patientID</th>

            <th scope="col">behaviour</th>
                                    <th scope="col">score</th>
                        
          
          </tr>
        </thead>

        <% for (let count = 0; count < givenValues.length; count++ ){ %>

        <tbody>
          <tr>
            <th scope="row"><%= count + 1%></th>
            <td><%= givenValues[count].recomenndedPhrases%></td>
            <td><%= givenValues[count].personalRedirection %></td>
                        <td><%= givenValues[count].patientID %></td>

            <td><%= givenValues[count].behaviour %></td>
             <td><%= givenValues[count].score %></td>
           


            <td><%= givenValues[count].comments %></td>
            <td class="text-center">
              <a
                href="/incoming-list/edit/<%= givenValues[count]._id %>"
                class="btn btn-primary"
              >
                <i class="fa fa-pencil" aria-hidden="true"></i> Edit
              </a>
            </td>

            <td class="text-center">
              <a
                href="/incoming-list/delete/<%= givenValues[count]._id %>"
                class="btn btn-danger"
              >
                <i class="fa fa-trash" aria-hidden="true"></i> delete
              </a>
            </td>
          </tr>
        </tbody>
        <% } %>
      </table>
    </div>
  </div>
</div>

<%include ../partials/footer.ejs %>
